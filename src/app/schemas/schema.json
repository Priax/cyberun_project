{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "description": "Veuillez entrer votre nom"
    },
    "countries": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "country": {
            "type": "string",
            "description": "Choisissez un pays",
            "enum": [
              "France",
              "Belgique",
              "Allemagne",
              "Burkina Faso",
              "Panama",
              "Inconnu"
            ]
          },
          "percentage": {
            "type": "number",
            "minimum": 0,
            "maximum": 100
          }
        },
        "required": ["country", "percentage"]
      }
    }
  },
  "required": ["name", "countries"]
}
